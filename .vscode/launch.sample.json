{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": [
                "--gitbucket_endpoint=endpoint",
                "--gitbucket_owner=owner",
                "--gitbucket_repo=repo",
                "--gitbucket_token=token"
            ]
        }
    ]
}